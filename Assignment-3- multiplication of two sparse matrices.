from scipy import sparse
from scipy.stats import uniform
import numpy as np
from scipy.sparse import coo_matrix

array1=[[0,0,0,1],
       [3,0,0,0],
       [4,0,0,0],
       [0,0,2,0]
      ]

i=[]
j=[]
k=[]

for q in range(len(array1)):
    for w in range(len(array1[0])):
        if array1[q][w]!=0:
            i.append(q)
            j.append(w)
            k.append(array1[q][w])

row  = np.array(i)
col  = np.array(j)
data = np.array(k)
t=coo_matrix((data, (row, col)))

array2=[[0,2,0,1],
       [3,7,0,0],
       [4,0,0,0],
       [0,0,6,0]
      ]

i=[]
j=[]
k=[]

for q in range(len(array2)):
    for w in range(len(array2[0])):
        if array2[q][w]!=0:
            i.append(q)
            j.append(w)
            k.append(array2[q][w])

row2  = np.array(i)
col2  = np.array(j)
data2 = np.array(k)
v=coo_matrix((data2, (row2, col2)))
print (coo_matrix.multiply(t,v))



